        #endregion


		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
        public void Main()
            {
                string logFilePath = @"C:\Timesheets\LeaveLog.txt";
                StringBuilder debugInfo = new StringBuilder();
                debugInfo.AppendLine($"[{DateTime.Now}]: Starting sheet detection");

                try
                {
                //string excelFilePath = Dts.Variables["User::ExcelFilePath"].Value.ToString();

                string excelFilePath = "C:\\Timesheets\\Siyakhanya Mjikeliso\\Siyakhanya Mjikeliso May Final Week.xlsx";
                    if (!File.Exists(excelFilePath))
                    {
                        debugInfo.AppendLine($"Excel file not found: {excelFilePath}");
                        File.AppendAllText(logFilePath, debugInfo.ToString());
                        Dts.TaskResult = (int)ScriptResults.Failure;
                        return;
                    }

                    string[] monthSheets = { "Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec" };
                    string fileName = Path.GetFileNameWithoutExtension(excelFilePath); // Filename
                    string detectedMonth = monthSheets.FirstOrDefault(m => fileName.IndexOf(m, StringComparison.OrdinalIgnoreCase) >= 0); // Check the month on the file

                    if (string.IsNullOrEmpty(detectedMonth)) // validate the month
                    {
                        debugInfo.AppendLine($"No month detected in filename: {fileName}");
                        File.AppendAllText(logFilePath, debugInfo.ToString());
                        Dts.TaskResult = (int)ScriptResults.Failure;
                        return;
                    }

                    string excelConnStr = $"Provider=Microsoft.ACE.OLEDB.12.0;Data Source={excelFilePath};Extended Properties=\"Excel 12.0 Xml;HDR=YES;IMEX=1\"";
                    bool forecastSheetFound = false;

                    using (OleDbConnection conn = new OleDbConnection(excelConnStr))
                    {
                        conn.Open();
                        debugInfo.AppendLine($"Processing file: {Path.GetFileName(excelFilePath)} - Month: {detectedMonth}");
                        debugInfo.AppendLine("-------------------------------");

                        DataTable dtSheets = conn.GetOleDbSchemaTable(OleDbSchemaGuid.Tables, null);
                        debugInfo.AppendLine("Sheets found in workbook:");

                        foreach (DataRow row in dtSheets.Rows)
                        {
                            string sheetName = row["TABLE_NAME"].ToString();
                            debugInfo.AppendLine($"- {sheetName}");

                            if (sheetName.Equals($"{detectedMonth}$", StringComparison.OrdinalIgnoreCase))
                            {
                                forecastSheetFound = true;
                                debugInfo.AppendLine($"Processing {detectedMonth} sheet for forecast data");

                                try
                                {
                                    // Read full sheet to detect forecast section
                                    var cmd = new OleDbCommand($"SELECT * FROM [{sheetName}]", conn);
                                    var adapter = new OleDbDataAdapter(cmd);
                                    var dt = new DataTable();
                                    adapter.Fill(dt);
                                    int currentRow = 0;

                                    foreach (DataRow dataRow in dt.Rows)
                                    {
                                        currentRow++;
                                        if (dataRow.ItemArray.Any(cell => cell?.ToString().ToLower().Contains("forecast hours") == true ||
                                                                         cell?.ToString().ToLower().Contains("billable hours") == true ||
                                                                         cell?.ToString().ToLower().Contains("non billable hours") == true ||
                                                                         cell?.ToString().ToLower().Contains("line manager") == true))
                                        {
                                            debugInfo.AppendLine($"Row {currentRow}: Forecast section detected.");
                                            break;
                                        }
                                        if (dataRow.ItemArray.Any(cell => cell?.ToString().ToLower().Contains("footer") == true))
                                        {
                                            debugInfo.AppendLine($"Row {currentRow}: Footer detected. Stopping.");
                                            break;
                                        }
                                    }

                                    // Set ranges based on detected month (e.g., Sep)
                                    string forecastRange1 = $"{sheetName}E40:F41"; // Forecasted Hours, Work Days
                                    string forecastRange2 = $"{sheetName}E43:F45"; // Billable, Non-Billable, Total
                       
                                var cmd1 = new OleDbCommand($"SELECT * FROM [{forecastRange1}]", conn);
                                    var adapter1 = new OleDbDataAdapter(cmd1);
                                    var dt1 = new DataTable();
                                    adapter1.Fill(dt1);
                                    var cmd2 = new OleDbCommand($"SELECT * FROM [{forecastRange2}]", conn);
                                    var adapter2 = new OleDbDataAdapter(cmd2);
                                    var dt2 = new DataTable();
                                    adapter2.Fill(dt2);

                                    if (dt1.Rows.Count == 0 || dt2.Rows.Count == 0)
                                    {
                                        debugInfo.AppendLine("No valid forecast data found in specified ranges.");
                                        Dts.Variables["User::ForecastQuery"].Value = "";
                                        Dts.Variables["User::ValidForecastSheetName"].Value = "";
                                        continue;
                                    }

                                    string dynamicSelectQuery = $"SELECT [F1] AS ForecastedHours, [F2] AS ForecastedWorkDays FROM [{forecastRange1}] " +
                                                               $"UNION ALL SELECT [F1] AS BillableHours, [F2] AS NonBillableHours FROM [{forecastRange2}] WHERE [F1] IS NOT NULL " +
                                                               $"UNION ALL SELECT NULL, [F2] AS TotalHours FROM [{forecastRange2}] WHERE [F1] IS NULL";
                                    Dts.Variables["User::ForecastQuery"].Value = dynamicSelectQuery;
                                    Dts.Variables["User::ValidForecastSheetName"].Value = sheetName;
                                    Dts.Variables["User::DetectedMonth"].Value = detectedMonth;

                                    debugInfo.AppendLine($"Set forecast query to: {dynamicSelectQuery}");
                                }
                                catch (Exception ex)
                                {
                                    debugInfo.AppendLine($"Error reading Forecast section: {ex.Message}");
                                    File.AppendAllText(logFilePath, debugInfo.ToString());
                                    Dts.TaskResult = (int)ScriptResults.Failure;
                                    return;
                                }
                            }
                        }

                        if (!forecastSheetFound)
                        {
                            debugInfo.AppendLine("No matching month sheet found for forecast data.");
                            Dts.Variables["User::ForecastQuery"].Value = "";
                            Dts.Variables["User::ValidForecastSheetName"].Value = "";
                        }

                        File.AppendAllText(logFilePath, debugInfo.ToString());
                        Dts.TaskResult = (int)ScriptResults.Success;
                    }
                }
                catch (Exception ex)
                {
                    debugInfo.AppendLine($"ERROR PROCESSING FILE: {ex.Message}\nStack Trace: {ex.StackTrace}");
                    File.AppendAllText(logFilePath, debugInfo.ToString());
                    Dts.TaskResult = (int)ScriptResults.Failure;
                }
            }

    #region ScriptResults declaration
