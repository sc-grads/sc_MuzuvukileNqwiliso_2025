      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{FAFBFD70-9B28-4D88-B61F-E690AB7A4F99}"
          SQLTask:SqlStatementSource="-- Step 1: Insert Activities (no 'leave' in name)&#xA;INSERT INTO Timesheet.Activity (ActivityName)&#xA;SELECT DISTINCT als.ActivityOrLeaveType&#xA;FROM Timesheet.ActivityLeaveStaging als&#xA;WHERE LOWER(als.ActivityOrLeaveType) NOT LIKE '%leave%'&#xA;  AND NOT EXISTS (&#xA;      SELECT 1 FROM Timesheet.Activity a&#xA;      WHERE a.ActivityName = als.ActivityOrLeaveType&#xA;  );&#xA;&#xA;-- Step 2: Optional - Log Activity inserts (simplified, assuming AuditLog exists)&#xA;IF OBJECT_ID('Timesheet.AuditLog', 'U') IS NOT NULL&#xA;BEGIN&#xA;    INSERT INTO Timesheet.AuditLog (FileName, TableName, Action, Message)&#xA;    SELECT &#xA;        als.FileName,&#xA;        'Activity' AS TableName,&#xA;        'Insert' AS Action,&#xA;        'New activity inserted: ' + als.ActivityOrLeaveType AS Message&#xA;    FROM Timesheet.ActivityLeaveStaging als&#xA;    WHERE LOWER(als.ActivityOrLeaveType) NOT LIKE '%leave%'&#xA;    GROUP BY als.FileName, als.ActivityOrLeaveType;&#xA;END;&#xA;&#xA;-- Step 3: Insert LeaveTypes (contains 'leave') - Optional if LeaveType table exists&#xA;IF OBJECT_ID('Timesheet.LeaveType', 'U') IS NOT NULL&#xA;BEGIN&#xA;    INSERT INTO Timesheet.LeaveType (LeaveTypeName)&#xA;    SELECT DISTINCT als.ActivityOrLeaveType&#xA;    FROM Timesheet.ActivityLeaveStaging als&#xA;    WHERE LOWER(als.ActivityOrLeaveType) LIKE '%leave%'&#xA;      AND NOT EXISTS (&#xA;          SELECT 1 FROM Timesheet.LeaveType lt&#xA;          WHERE lt.LeaveTypeName = als.ActivityOrLeaveType&#xA;      );&#xA;END;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
