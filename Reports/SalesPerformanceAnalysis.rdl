  <DataSets>
    <DataSet Name="SalesByStore">
      <Query>
        <DataSourceName>BikeStoresDS</DataSourceName>
        <CommandText>SELECT 
    s.store_name,
    SUM(oi.quantity * oi.list_price) AS total_sales
FROM sales.stores s
JOIN sales.orders o ON s.store_id = o.store_id
JOIN sales.order_items oi ON o.order_id = oi.order_id
GROUP BY s.store_name;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="store_name">
          <DataField>store_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="total_sales">
          <DataField>total_sales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="SalesByCategory">
      <Query>
        <DataSourceName>BikeStoresDS</DataSourceName>
        <CommandText>SELECT 
    c.category_name,
    SUM(oi.quantity * oi.list_price) AS category_sales
FROM production.categories c
JOIN production.products p ON c.category_id = p.category_id
JOIN sales.order_items oi ON p.product_id = oi.product_id
GROUP BY c.category_name;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="category_name">
          <DataField>category_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="category_sales">
          <DataField>category_sales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="MonthlySales">
      <Query>
        <DataSourceName>BikeStoresDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@YearParam">
            <Value>=Parameters!YearParam.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
    YEAR(o.order_date) AS sale_year,
    MONTH(o.order_date) AS sale_month,
    SUM(oi.quantity * oi.list_price) AS monthly_sales
FROM sales.orders o
JOIN sales.order_items oi ON o.order_id = oi.order_id
WHERE 
   (@YearParam IS NULL OR YEAR(o.order_date) = @YearParam)
GROUP BY YEAR(o.order_date), MONTH(o.order_date)
ORDER BY sale_year, sale_month;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="sale_year">
          <DataField>sale_year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="sale_month">
          <DataField>sale_month</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="monthly_sales">
          <DataField>monthly_sales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="TopProducts">
      <Query>
        <DataSourceName>BikeStoresDS</DataSourceName>
        <CommandText>SELECT TOP 10
    p.product_name,
    c.category_name,
    SUM(oi.quantity) AS units_sold,
    SUM(oi.quantity * oi.list_price) AS total_sales
FROM production.products p
JOIN production.categories c ON p.category_id = c.category_id
JOIN sales.order_items oi ON p.product_id = oi.product_id
GROUP BY p.product_name, c.category_name
ORDER BY total_sales DESC;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="product_name">
          <DataField>product_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="category_name">
          <DataField>category_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="units_sold">
          <DataField>units_sold</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="total_sales">
          <DataField>total_sales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="AvgOrderValue">
      <Query>
        <DataSourceName>BikeStoresDS</DataSourceName>
        <CommandText>SELECT 
    s.store_name,
    AVG(oi.quantity * oi.list_price) AS avg_order_value
FROM sales.stores s
JOIN sales.orders o ON s.store_id = o.store_id
JOIN sales.order_items oi ON o.order_id = oi.order_id
GROUP BY s.store_name;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="store_name">
          <DataField>store_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="avg_order_value">
          <DataField>avg_order_value</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="AvailableYears">
      <Query>
        <DataSourceName>BikeStoresDS</DataSourceName>
        <CommandText>SELECT 
    YearValue,
    CASE WHEN YearValue IS NULL THEN 'All Years' 
         ELSE CAST(YearValue AS VARCHAR(4)) END AS YearLabel
FROM (
    SELECT DISTINCT YEAR(order_date) AS YearValue FROM sales.orders
    UNION SELECT NULL
) AS Years
ORDER BY YearValue DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="YearValue">
          <DataField>YearValue</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="YearLabel">
          <DataField>YearLabel</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
